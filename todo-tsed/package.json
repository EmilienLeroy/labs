{
  "name": "todo-tsed",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "yarn tsc",
    "tsc": "tsc --project tsconfig.compile.json",
    "tsc:w": "tsc --project tsconfig.json -w",
    "start": "nodemon --watch \"src/**/*.ts\" --ignore \"node_modules/**/*\" --exec ts-node src/index.ts",
    "start:prod": "cross-env NODE_ENV=production node dist/index.js"
  },
  "dependencies": {
    "@tsed/ajv": "^6.23.1",
    "@tsed/common": "^6.23.1",
    "@tsed/core": "^6.23.1",
    "@tsed/di": "^6.23.1",
    "@tsed/exceptions": "^6.23.1",
    "@tsed/json-mapper": "^6.23.1",
    "@tsed/platform-express": "^6.23.1",
    "@tsed/schema": "^6.23.1",
    "ajv": "^7.0.3",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "express": "^4.17.1",
    "method-override": "^3.0.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.11",
    "@types/method-override": "0.0.31",
    "@types/multer": "^1.4.5",
    "@types/node": "^14.14.22",
